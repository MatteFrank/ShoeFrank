/*!
  \file
  \version $Id: TAGdataDsc.icc,v 1.4 2001/11/18 18:49:15 mueller Exp $
  \brief   Implementation of (inline) TAGdataDsc.
*/

//------------------------------------------+-----------------------------------
//! Returns pointer to data object.

inline TAGdata* TAGdataDsc::Object() const
{
  return fpObject;
}

//------------------------------------------+-----------------------------------
//! Returns pointer to data object.

inline TAGdata** TAGdataDsc::ObjectPointer()
{
  return &fpObject;
}

//------------------------------------------+-----------------------------------
//! Returns pointer to TClass of data object.

inline TClass* TAGdataDsc::ObjectClass() const
{
  return fpObjectClass;
}

//------------------------------------------+-----------------------------------
//! Returns pointer to action generating the data object

inline TAGaction* TAGdataDsc::Producer() const
{
  return fpProducer;
}

//------------------------------------------+-----------------------------------
//! Returns setting of the kValid status bit.

inline Bool_t TAGdataDsc::Valid() const
{
  return TestBit(kValid);
}

//------------------------------------------+-----------------------------------
//! Returns setting of the kEof status bit.

inline Bool_t TAGdataDsc::Eof() const
{
  return TestBit(kEof);
}

