/*!
  \file
  \version $Id: TAGnamed.icc,v 1.2 2001/10/28 20:33:34 mueller Exp $
  \brief   Implementation (inline) of TAGnamed.
*/

//------------------------------------------+-----------------------------------
//! Default constructor.

inline TAGnamed::TAGnamed()
: fDebugLevel(0)
{}

//------------------------------------------+-----------------------------------
//! Construct with \a name and \a title.

inline TAGnamed::TAGnamed(const char* name, const char* title)
  : TNamed(name, title)
{}

//------------------------------------------+-----------------------------------
//! Construct with \a name and \a title.

inline TAGnamed::TAGnamed(const TString& name, const TString& title)
  : TNamed(name, title)
{}

//------------------------------------------+-----------------------------------
//! Returns setting of the kFail status bit.

inline Bool_t TAGnamed::Fail() const
{
  return TestBit(kFail);
}

//------------------------------------------+-----------------------------------
//! Returns debug level

inline Int_t TAGnamed::GetDebugLevel() const
{
   return fDebugLevel;
}

//------------------------------------------+-----------------------------------
//! Set debug level
inline void  TAGnamed::SetDebugLevel(Int_t d)
{
   fDebugLevel = d;
}

//------------------------------------------+-----------------------------------
/*!
  \relates TAGnamed
  \brief ostream insertion operator.
*/

inline ostream& operator<<(ostream& os, const TAGnamed& obj)
{
  obj.ToStream(os);
  return os;
}

