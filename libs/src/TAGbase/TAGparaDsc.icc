//------------------------------------------+-----------------------------------
//! Returns pointer to para object.

inline TAGpara* TAGparaDsc::Object() const
{
  return fpObject;
}

//------------------------------------------+-----------------------------------
//! Returns pointer to para object.

inline TAGpara** TAGparaDsc::ObjectPointer()
{
  return &fpObject;
}

//------------------------------------------+-----------------------------------
//! Returns pointer to TClass of para object.

inline TClass* TAGparaDsc::ObjectClass() const
{
  return fpObjectClass;
}

//------------------------------------------+-----------------------------------
//! Returns setting of the kValid status bit.

inline Bool_t TAGparaDsc::Valid() const
{
  return TestBit(kValid);
}

