/*!
  \file
  \version $Id: TAGpara.icc,v 1.2 2003/06/09 18:37:39 mueller Exp $
  \brief   Implementation of (inline) TAGpara.
*/

//------------------------------------------+-----------------------------------
//! Default constructor.

inline TAGpara::TAGpara()
{}

//------------------------------------------+-----------------------------------
//! Returns setting of the kFail status bit.

inline Bool_t TAGpara::Fail() const
{
  return TestBit(kFail);
}

//------------------------------------------+-----------------------------------
//! Returns debug level

inline Int_t TAGpara::GetDebugLevel() const
{
   return fDebugLevel;
}

//------------------------------------------+-----------------------------------
//! Set debug level
inline void  TAGpara::SetDebugLevel(Int_t d)
{
   fDebugLevel = d;
}

//------------------------------------------+-----------------------------------
/*!
  \relates TAGpara
  \brief ostream insertion operator.
*/

inline ostream& operator<<(ostream& os, const TAGpara& obj)
{
  obj.ToStream(os);
  return os;
}

