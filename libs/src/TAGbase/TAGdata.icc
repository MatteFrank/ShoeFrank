/*!
  \file
  \version $Id: TAGdata.icc,v 1.3 2003/06/09 18:37:01 mueller Exp $
  \brief   Implementation of (inline) TAGdata.
*/

//------------------------------------------+-----------------------------------
//! Default constructor.

inline TAGdata::TAGdata()
{}

//------------------------------------------+-----------------------------------
//! Returns setting of the kFail status bit.

inline Bool_t TAGdata::Fail() const
{
  return TestBit(kFail);
}

//------------------------------------------+-----------------------------------
/*!
  \relates TAGdata
  \brief ostream insertion operator.
*/

inline ostream& operator<<(ostream& os, const TAGdata& obj)
{
  obj.ToStream(os);
  return os;
}

