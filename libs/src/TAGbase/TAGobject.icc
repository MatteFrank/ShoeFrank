/*!
  \file
  \version $Id: TAGobject.icc,v 1.2 2001/10/28 20:33:34 mueller Exp $
  \brief   Implementation of (inline) TAGobject.
*/

//------------------------------------------+-----------------------------------
//! Default constructor.

inline TAGobject::TAGobject()
: fDebugLevel(0),
  fFound(false)
{}

//------------------------------------------+-----------------------------------
//! Returns setting of the kFail status bit.

inline Bool_t TAGobject::Fail() const
{
  return TestBit(kFail);
}

//------------------------------------------+-----------------------------------
//! Returns debug level

inline Int_t TAGobject::GetDebugLevel() const
{
   return fDebugLevel;
}

//------------------------------------------+-----------------------------------
//! Set debug level
inline void  TAGobject::SetDebugLevel(Int_t d)
{
   fDebugLevel = d;
}

//------------------------------------------+-----------------------------------
//! Returns found

inline Bool_t TAGobject::Found() const
{
   return  fFound;
}

//------------------------------------------+-----------------------------------
//! Set debug level
inline void  TAGobject::SetFound(Bool_t b)
{
   fFound = b;
}

//------------------------------------------+-----------------------------------
/*!
  \relates TAGobject
  \brief ostream insertion operator.
*/

inline ostream& operator<<(ostream& os, const TAGobject& obj)
{
  obj.ToStream(os);
  return os;
}

