//------------------------------------------+-----------------------------------
//! Access \a i 'th track

inline TABMntuTrackTr* TABMntuTrack::Track(Int_t i)
{
  if(GetTracksN())
    return (TABMntuTrackTr*) ((*fListOfTracks)[i]);
  else 
    return 0x0;
}

//------------------------------------------+-----------------------------------
//! Read-only access \a i 'th track

inline const TABMntuTrackTr* TABMntuTrack::Track(Int_t i) const
{
  if(GetTracksN())
    return (const TABMntuTrackTr*) ((*fListOfTracks)[i]);
  else 
    return 0x0;;
}

//------------------------------------------+-----------------------------------
//! return number of tracks
inline Int_t TABMntuTrack::GetTracksN() const
{
   return fListOfTracks->GetEntries();
}


//------------------------------------------+-----------------------------------
//! return tracks status
inline Int_t& TABMntuTrack::GetTrackStatus()
{
   return fStatus;
}

