//------------------------------------------+-----------------------------------
//! Returns Cell number [0-2]

inline Int_t TABMntuHit::Cell() const
{
  return icell;
}

//------------------------------------------+-----------------------------------
//! Returns X,Y,Z and dist 

inline Double_t TABMntuHit::Dist() const
{
  return rdrift;
}

inline Double_t TABMntuHit::Tdrift() const
{
  return tdrift;
}

//------------------------------------------+-----------------------------------
//! Returns Plane number [0-5].

inline Int_t TABMntuHit::Plane() const
{
  return ilayer;
}

//------------------------------------------+-----------------------------------
//! Returns View number [0 or 1].

inline Int_t TABMntuHit::View() const
{
  return iview;
}
